# Copy this file to .env and adjust values as needed
MONGODB_URI=mongodb://root:example@localhost:27017/?authSource=admin
# Optional: Postgres for persistent conversation state (Prisma)
# Example: postgresql://postgres:postgres@localhost:5442/postgres
AGENTS_DATABASE_URL=

# Optional: HashiCorp Vault (dev)
# Enable to use Vault-backed env vars and autocomplete endpoints
VAULT_ENABLED=false
VAULT_ADDR=http://localhost:8200
VAULT_TOKEN=dev-root
